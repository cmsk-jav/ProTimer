<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="closePane" prefHeight="75.0" prefWidth="260.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sk.controller.FXMLController">
   <children>
      <Label id="closeWarning_lbl" fx:id="closeWarning_lbl" alignment="CENTER" contentDisplay="CENTER" prefHeight="42.0" prefWidth="260.0" styleClass="box" text="ARE YOU SURE?">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Button id="yes_btn" fx:id="yes_btn" layoutX="65.0" layoutY="43.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="25.0" prefWidth="65.0" styleClass="box" text="YES">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button id="no_btn" fx:id="no_bt" layoutX="138.0" layoutY="43.0" mnemonicParsing="false" onAction="#cancelCloseWindow" prefHeight="25.0" prefWidth="65.0" styleClass="box" text="NO">
         <font>
            <Font size="23.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
